pull_by_act <- function(act, mood){
el <- list()
for (j in 1:nrow(full_data)) {
el[[j]] <- list()
}
df <- full_data %>%
subset(Act == act)
# Loop over rows in data frame
for (i in 1:nrow(df)) {
# Check if 'doctor' is present in occupation column for this row
if (grepl(moods_names[[mood]], df[i, "Mood"])) {
# If it is, add this row to the list of matching rows
el[i] <- df[i, "Track.Name"]
}
}
el <- remove_empty_lists(el)
el <- paste(el)
el <- str_remove(el, "\\\\")
return(el)
}
